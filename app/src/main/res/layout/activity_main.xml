<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Welcome Snake App"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/btnLoadAndShowInter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test Ads Inter LoadAndShow"
            android:padding="@dimen/_5sdp"
            android:textColor="@color/white"
            android:background="@color/black"
            android:layout_marginTop="@dimen/_10sdp"/>
        <TextView
            android:id="@+id/btnShowInter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test Ads Inter"
            android:padding="@dimen/_5sdp"
            android:textColor="@color/white"
            android:background="@color/black"
            android:layout_marginTop="@dimen/_10sdp"/>

        <TextView
            android:id="@+id/btnLoadNativeFullScreen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test Native Full Screen"
            android:padding="@dimen/_5sdp"
            android:textColor="@color/white"
            android:background="@color/black"
            android:layout_marginTop="@dimen/_10sdp"/>



        <TextView
            android:id="@+id/btnShowRewardedAds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test Ads Reward"
            android:padding="@dimen/_5sdp"
            android:textColor="@color/white"
            android:background="@color/black"
            android:layout_marginTop="@dimen/_10sdp"/>

    </LinearLayout>

</RelativeLayout>
